@model List<PeopleViewModel>
@{
}







<table id="myTable">
    <thead>

    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
    </tr>
    </thead>
    <tbody>

    <@foreach (var person in Model)
    {
        <tr>
            <td>
                @person.Id
            </td>
            <td>
                @person.Name
            </td>

        </tr>
    }
    </tbody>
</table>
